{
  "name": "alphapebble",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build-data": "node scripts/build-data.js",
    "build:worker": "next build && node scripts/ensure-pages-manifest.cjs && node scripts/patch-standalone-node-mods.js && node scripts/remove-sharp.js && npx @opennextjs/cloudflare build --skip-build",
    "predev": "node -e \"const p=require('node:path'); const fs=require('node:fs'); fs.writeFileSync('.dev.vars', 'OPENNEXT_SERVER_WORKING_DIRECTORY='+p.resolve('.open-next/server-functions/default')+'\\n');\" && npm run build:worker",
    "dev": "wrangler dev .open-next/worker.js --assets .open-next/assets",
    "dev:next": "next dev",
    "build": "npm run build-data && next build && node scripts/ensure-pages-manifest.cjs && node scripts/patch-standalone-node-mods.js && node scripts/remove-sharp.js && npx @opennextjs/cloudflare build --skip-build",
    "build:next-only": "next build",
    "start": "next start",
    "deploy": "npm run build && wrangler deploy",
    "deploy:preview": "npm run build && wrangler deploy --env preview",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:check": "npx audit-ci --moderate",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "cross-env ANALYZE=true npm run build:next-only",
    "lighthouse": "lhci autorun",
    "perf:analyze": "npm run analyze && npm run lighthouse",
    "test:build": "npm run build:next-only && npm start",
    "test:csp": "./scripts/test-csp.sh",
    "test:legal": "./scripts/test-legal-pages.sh",
    "clean": "rm -rf .next .open-next dist out"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^15.5.4",
    "@types/mdx": "^2.0.13",
    "aos": "^2.3.4",
    "class-variance-authority": "^0.7.1",
    "gray-matter": "^4.0.3",
    "next": "15.5.2",
    "next-mdx-remote": "^5.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rss": "^1.2.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.12.0",
    "@next/bundle-analyzer": "^15.5.5",
    "@opennextjs/cloudflare": "^1.8.5",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/aos": "^3.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/rss": "^0.0.32",
    "audit-ci": "^7.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "open-next": "^3.1.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-breaks": "^4.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "unified": "^11.0.5",
    "wrangler": "^4.40.1"
  }
}
