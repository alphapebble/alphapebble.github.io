{
  "name": "alphapebble",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:worker": "next build && opennext build --platform=cloudflare",
    "deploy": "wrangler deploy",
    "dev:worker": "wrangler dev --local",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:check": "npx audit-ci --moderate",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "cross-env ANALYZE=true npm run build",
    "lighthouse": "lhci autorun"
  },
  "dependencies": {
    "@next/mdx": "^15.5.3",
    "aos": "^2.3.4",
    "class-variance-authority": "^0.7.1",
    "gray-matter": "^4.0.3",
    "next": "15.5.2",
    "next-mdx-remote": "^5.0.0",
    "plaiceholder": "^3.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "rss": "^1.2.2",
    "sharp": "^0.34.3",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.12.0",
    "@opennextjs/cloudflare": "^1.8.3",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/aos": "^3.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/rss": "^0.0.32",
    "audit-ci": "^7.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.38.0"
  }
}
